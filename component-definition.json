          "id": "accordion",
          "plugins": {
            "xwalk": {
              "page": {
                "resourceType": "core/franklin/components/block/v1/block",
                "template": {
                  "name": "Accordion",
                  "filter": "accordion"
                }
              }
            }
          }
        },
        {
          "title": "Accordion Item",
          "id": "accordion-item",
          "plugins": {
            "xwalk": {
              "page": {
                "resourceType": "core/franklin/components/block/v1/block/item",
                "template": {
                  "name": "Accordion Item",
                  "model": "accordion-item",
                  "summary": "Lorem Ipsum",
                  "text": "<p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Curabitur volutpat consequat dui, sit amet ...</p>"
                }
              }
            }
          }
        },
        {
          "title": "Cards",
          "id": "cards",
          "plugins": {
            "da": {
              "name": "cards",
              "rows": 1,
              "columns": 2
            }
          },
          "model": "cards",
          "filter": "cards"
        },
        {
          "title": "Card",
          "id": "card",
          "plugins": {
            "da": {
              "name": "card",
              "rows": 2,
              "columns": 0
