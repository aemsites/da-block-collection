{
  "groups": [
    {
      "title": "Default Content",
      "id": "default",
      "components": [
        {
          "title": "Text",
          "id": "text",
          "model": "text",
          "plugins": {
            "da": {
              "name": "text",
              "type": "text"
            }
          }
        },
        {
          "title": "Image",
          "id": "image",
          "model": "image",
          "plugins": {
            "da": {
              "name": "image",
              "type": "image"
            }
          }
        }
      ]
    },
    {
      "title": "Sections",
      "id": "sections",
      "components": [
        {
          "title": "Section",
          "id": "section",
          "plugins": {
            "da": {
              "unsafeHTML": "<div></div>"
            }
          },
          "filter": "section",
          "model": "section"
        }
      ]
    },
    {
      "title": "Blocks",
      "id": "blocks",
      "components": [
        {
          "title": "Accordion",
          "id": "accordion",
          "model": "accordion",
          "filter": "accordion",
          "plugins": {
            "da": {
              "name": "accordion",
              "rows": 1,
              "columns": 2,
              "fields": [
                {
                  "summary": "div:nth-child(1)",
                  "text": "div:nth-child(2)"
                }
              ]
            }
          }
        },
        {
          "title": "Accordion Item",
          "id": "accordion-item",
          "model": "accordion-item",
          "plugins": {
            "da": {
              "name": "accordion-item",
              "rows": 2,
              "columns": 0
            }
          }
        },
        {
          "title": "Cards",
          "id": "cards",
          "model": "cards",
          "filter": "cards",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"cards\"><div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture></div><div></div></div></div>"
            }
          }
        },
        {
          "title": "Card",
          "id": "card",
          "model": "card",
          "plugins": {
            "da": {
              "unsafeHTML": "<div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture></div><div></div></div>",
              "fields": [
                {
                  "image": "div:nth-child(1)>picture:nth-child(1)>img[src]",
                  "imageAlt": "div:nth-child(1)>picture:nth-child(1)>img[alt]",
                  "text": "div:nth-child(2)"
                }
              ]
            }
          }
        },
        {
          "title": "Carousel",
          "id": "carousel",
          "model": "carousel",
          "filter": "carousel",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"carousel\"><div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture></div><div></div></div>"
            }
          }
        },
        {
          "title": "Carousel Slide",
          "id": "carousel-item",
          "model": "carousel-item",
          "plugins": {
            "da": {
              "unsafeHTML": "<div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture></div><div></div></div>",
              "fields": [
                {
                  "backgroundImage": "div:nth-child(1)>picture:nth-child(1)>img[src]",
                  "backgroundImageAlt": "div:nth-child(1)>picture:nth-child(1)>img[alt]",
                  "text": "div:nth-child(2)"
                }
              ]
            }
          }
        },
        {
          "title": "Columns",
          "id": "columns",
          "model": "columns",
          "filter": "columns",
          "plugins": {
            "da": {
              "name": "columns",
              "rows": 1,
              "columns": 2,
              "behaviour": "columns"
            }
          }
        },
        {
          "title": "Columns Row",
          "id": "columns-row",
          "model": "columns-row",
          "filter": "columns-row",
          "plugins": {
            "da": {
              "name": "columns-row",
              "behaviour": "columns-row"
            }
          }
        },
        {
          "title": "Columns Cell",
          "id": "columns-cell",
          "model": "columns-cell",
          "filter": "columns-cell",
          "plugins": {
            "da": {
              "unsafeHTML": "<div></div>",
              "behaviour": "columns-cell"
            }
          }
        },
        {
          "title": "Fragment",
          "id": "fragment",
          "model": "fragment",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"fragment\"><div><div><p><a href=\"\">Fragment URL</a></p></div></div></div>",
              "fields": [
                {
                  "url": "div:nth-child(1)>div:nth-child(1)>p:nth-child(1)>a:nth-child(1)[href]",
                  "text": "div:nth-child(1)>div:nth-child(1)>p:nth-child(1)>a:nth-child(1)"
                }
              ]
            }
          }
        },
        {
          "title": "Hero",
          "id": "hero",
          "model": "hero",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"hero\"><div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture><h1></h1></div></div></div>",
              "fields": [
                {
                  "image": "div:nth-child(1)>div:nth-child(1)>picture>img[src]",
                  "imageAlt": "div:nth-child(1)>div:nth-child(1)>picture>img[alt]",
                  "text": "div:nth-child(1)>div:nth-child(1)>h1"
                }
              ]
            }
          }
        },
        {
          "title": "Quote",
          "id": "quote",
          "model": "quote",
          "plugins": {
            "da": {
              "name": "quote",
              "rows": 1,
              "columns": 1,
              "fields": [
                {
                  "quote": "div:nth-child(1)>div",
                  "author": "div:nth-child(2)>div>p"
                }
              ]
            }
          }
        },
        {
          "title": "Search",
          "id": "search",
          "model": "search",
          "plugins": {
            "da": {
              "name": "search",
              "rows": 1,
              "columns": 1,
              "fields": [
                {
                  "searchUrl": "div:nth-child(1)>div:nth-child(1)>p:nth-child(1)>a[href]",
                  "searchUrlText": "div:nth-child(1)>div:nth-child(1)>p:nth-child(1)>a"
                }
              ]
            }
          }
        },
        {
          "title": "Tabs",
          "id": "tabs",
          "model": "tabs",
          "filter": "tabs",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"tabs\"><div><div><p></p></div><div></div></div></div>"
            }
          }
        },
        {
          "title": "Tabs Item",
          "id": "tabs-item",
          "model": "tabs-item",
          "plugins": {
            "da": {
              "unsafeHTML": "<div><div><p></p></div><div></div></div>",
              "fields": [
                {
                  "name": "div:nth-child(1)>p",
                  "content": "div:nth-child(2)"
                }
              ]
            }
          }
        },
        {
          "title": "Video",
          "id": "video",
          "model": "video",
          "plugins": {
            "da": {
              "name": "video",
              "rows": 1,
              "columns": 1,
              "fields": [
                {
                  "videoUrl": "div:nth-child(1)>div:nth-child(1)>p:nth-child(1)>a[href]",
                  "videoUrlText": "div:nth-child(1)>div:nth-child(1)>p:nth-child(1)>a",
                  "posterImage": "div:nth-child(1)>div:nth-child(1)>picture>img[src]"
                }
              ]
            }
          }
        }
      ]
    }
  ]
}